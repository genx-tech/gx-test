{
  "name": "@genx/test",
  "version": "1.1.4",
  "description": "Gen-X test framework",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@genx/app": "*",
    "rk-utils": "*"
  },
  "dependencies": {
    "allure-commandline": "^2.13.0",
    "allure-mocha": "^2.0.0-beta.8",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.3",
    "mocha-multi": "^1.1.3",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "supertest": "^5.0.0"
  },
  "devDependencies": {
    "ink-docstrap": "^1.3.2"
  },
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs' -r --verbose ./src",
    "bulid:clean": "rm -rf lib",
    "build": "npm run bulid:clean && NODE_ENV=production babel src -d lib --copy-files && del lib/**/__test__"
  },
  "author": "Rockie Guo <rockie@kitmi.com.au>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genx-tech/gx-test.git"
  },
  "bugs": {
    "url": "https://github.com/genx-tech/gx-test/issues"
  },
  "keywords": [
    "genx-suite",
    "test"
  ]
}
